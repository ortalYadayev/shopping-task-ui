{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"plugin:vue/vue3-recommended",
		"airbnb-base",
		"@vue/eslint-config-prettier",
		"prettier"
	],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"vue",
	],
	"rules": {
		"prettier/prettier": "error",
		"vue/no-unused-vars": "error"
	}
}
